# AWS IAM Role Policy for School Portal S3 Access
{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "SchoolPortalS3Access",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:PutObjectAcl",
        "s3:GetObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::cecre-logs-bucket-v1",
        "arn:aws:s3:::cecre-logs-bucket-v1/*"
      ]
    }
  ]
}
